                            <TableColumnItem>
                                <PropertyName>_udfs</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>
        <!--CosmosDb.Collection.WideView -->
