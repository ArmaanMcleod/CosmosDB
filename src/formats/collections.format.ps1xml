                    <WideEntry>
                        <WideItem>
                            <PropertyName>id</PropertyName>
                        </WideItem>
                    </WideEntry>
                </WideEntries>
            </WideControl>
        </View>
        <!--CosmosDb.Collection.ListView-->
        <View>
            <Name>CosmosDb.Collection.ListView</Name>
            <ViewSelectedBy>
                <TypeName>CosmosDb.Collection</TypeName>
            </ViewSelectedBy>
